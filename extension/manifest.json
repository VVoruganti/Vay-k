{
    "manifest_version": 2,
  
    "name": "Vay-K",
    "description": "Plan trips easily with the Vay-K Chrome Extension.",
    "version": "1.0",
    "icons": { "128": "src/images/logo.png" },
    "browser_action": {
      "default_icon": "src/images/logo.png",
      "default_popup": "src/popup.html"
    },
    "permissions": ["activeTab"],
    "content_security_policy": "script-src 'self'; object-src 'self'"
  }
