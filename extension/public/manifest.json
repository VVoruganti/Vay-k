{
    "manifest_version": 3,
    "name": "Vay-K",
    "description": "Plan trips easily with the Vay-K Chrome Extension.",
    "version": "1.0",
    "icons": { "128": "images/logo.png" },
    "action": {
      "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab", "contextMenus"]
  }
